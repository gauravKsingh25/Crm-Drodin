{
  "name": "crm-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/assets/crm/frontend/ && npm run copy-html-entry",
    "copy-html-entry": "cp ../crm/public/frontend/index.html ../crm/www/crm.html || echo 'HTML copy skipped'",
    "serve": "vite preview",
    "start": "npm run serve"
